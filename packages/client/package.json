{
  "name": "@based/client",
  "version": "2.7.5",
  "license": "MIT",
  "browser": {
    "./dist/file/stream/index.js": "./dist/file/stream/browser.js"
  },
  "type": "module",
  "source": "src/index.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.modern.js"
  },
  "mainFields": [
    "browser",
    "module",
    "main"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "scripts": {
    "build": "microbundle",
    "watch": "microbundle watch",
    "clean": "rimraf {.turbo,dist,node_modules}",
    "test": "ava --timeout 1m --verbose",
    "watchTest": "ava --color --watch --verbose"
  },
  "devDependencies": {
    "microbundle": "^0.15.0",
    "@types/node": "^17.0.23",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "rimraf": "^3.0.2",
    "concurrently": "^7.1.0",
    "esbuild": "^0.14.29",
    "@saulx/hash": "^1.1.0",
    "@saulx/selva": "^11.0.0",
    "@saulx/utils": "^2.2.1",
    "@saulx/selva-server": "11.0.0",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/ws": "^7.4.6",
    "ava": "4.2.0",
    "jsonwebtoken": "^8.5.1"
  },
  "sideEffects": false,
  "ava": {
    "files": [
      "test/*.ts"
    ],
    "concurrency": 1,
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "dependencies": {
    "@based/get-service": "2.4.3",
    "@based/graphql": "2.4.2",
    "@based/types": "^2.6.0",
    "@saulx/diff": "^1.1.3",
    "@saulx/hash": "^1.1.0",
    "@saulx/utils": "^2.2.1",
    "cross-fetch": "^3.1.4",
    "isomorphic-ws": "^4.0.1",
    "ws": "^7.5.3"
  }
}
